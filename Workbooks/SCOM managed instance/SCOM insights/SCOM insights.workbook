          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "alertsmanagementresources\r\n| where todatetime(properties.essentials.startDateTime) {timeRange}\r\n| where type == \"microsoft.alertsmanagement/alerts\"\r\n| where properties.essentials.monitorService contains \"SCOM Managed Instance\"\r\n//| project tostring('{InstanceName}')\r\n| where tostring(properties['essentials']['targetResource']) contains(tostring('{InstanceName}'))\r\n//| summarize count() by tostring(properties['essentials']['targetResource'])\r\n| summarize count() by Time = format_datetime(bin(todatetime(properties['essentials']['startDateTime']), 1d), 'MM-dd-yyyy'), State = tostring(properties['essentials']['alertState'])\r\n| order by Time asc\r\n\r\n//| render barchart",
              "size": 0,
              "title": "Alerts generated by day",
              "noDataMessage": "There are no alerts generated in the given time range",
              "noDataMessageStyle": 3,
              "queryType": 1,
              "resourceType": "microsoft.resourcegraph/resources",
              "crossComponentResources": [
                "value::all"
              ],
              "visualization": "barchart",
              "chartSettings": {
                "xAxis": "Time",
                "yAxis": [
                  "count_"
                ],
                "group": "State",
                "createOtherGroup": 0,
                "showMetrics": false,
                "showLegend": true,
                "seriesLabelSettings": [
                  {
                    "seriesName": "New",
                    "color": "blue"
                  },
                  {
                    "seriesName": "Closed",
                    "color": "green"
                  }
                ]
              }
            },
            "name": "Alerts by day"
          },
          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "AlertsManagementResources | where type =~ 'microsoft.alertsmanagement/alerts'\r\n| where todatetime(properties.essentials.startDateTime) {timeRange}\r\n| where properties.essentials.monitorService contains \"SCOM Managed Instance\"\r\n| where tostring(tolower(properties['essentials']['targetResource'])) contains (tolower(tostring('{InstanceName}')))\r\n| extend State=tostring(properties.essentials.alertState)\r\n| summarize Count=count(), New=countif(State==\"New\"), \r\nAcknowledged=countif(State==\"Acknowledged\"), \r\nClosed=countif(State==\"Closed\") \r\nby Severity=tostring(properties.essentials.severity)\r\n| order by Severity asc",
              "size": 3,
              "title": "Alert summary",
              "noDataMessage": "There are no alerts in the given time range",
              "noDataMessageStyle": 3,
              "exportMultipleValues": true,
              "exportedParameters": [
                {
                  "fieldName": "Severity",
                  "parameterName": "Severity",
                  "parameterType": 1
                }
              ],
              "queryType": 1,
              "resourceType": "microsoft.resourcegraph/resources",
              "crossComponentResources": [
                "value::all"
              ],
              "gridSettings": {
                "formatters": [
                  {
                    "columnMatch": "Severity",
                    "formatter": 11
                  },
                  {
                    "columnMatch": "Count",
                    "formatter": 3,
                    "formatOptions": {
